        const t = ['Aditya','pavan','Bannu']
        for(let i=0; i<t.length; i++){      //i<3
            let tt = document.createElement("h1")
            tt.className = "a"
            tt.textContent=t[i];
          //  console.log(t[i])
            console.log(tt)
